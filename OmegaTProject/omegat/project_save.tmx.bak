    <tu>
      <tuv lang="EN-US">
        <seg>The policy must be internally communicated.</seg>
      </tuv>
      <tuv lang="FR" changeid="camille" changedate="20190628T151428Z" creationid="camille" creationdate="20190628T151428Z">
