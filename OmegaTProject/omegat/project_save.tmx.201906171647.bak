    <tu>
      <tuv lang="EN-US">
        <seg>Appendix I: Language Translations</seg>
      </tuv>
      <tuv lang="FR" changeid="lionel" changedate="20190605T144529Z" creationid="lionel" creationdate="20190605T144444Z">
